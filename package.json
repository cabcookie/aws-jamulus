{
  "name": "cdk",
  "version": "1.6.4",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "quick": "`node fetch-ip.js ssh daw`",
    "ssh:band": "`node fetch-command.js ssh band`",
    "ssh:daw": "`node fetch-command.js ssh daw`",
    "ssh:mixer": "`node fetch-command.js ssh mixer`",
    "logs:band": "`node fetch-command.js logs band`",
    "logs:daw": "`node fetch-command.js logs daw`",
    "logs:mixer": "`node fetch-command.js logs mixer`",
    "ardour": "rm -rf temp/mosaik-live && `node fetch-command.js ardour daw`",
    "release": "standard-version",
    "major-release": "standard-version --release-as major",
    "push": "git push --follow-tags origin main",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.103.0",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "10.17.27",
    "aws-cdk": "^1.103.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.103.0",
    "@aws-cdk/aws-ec2": "^1.103.0",
    "@aws-cdk/aws-iam": "^1.103.0",
    "@aws-cdk/aws-lambda": "^1.103.0",
    "@aws-cdk/aws-logs": "^1.103.0",
    "@aws-cdk/aws-s3": "^1.103.0",
    "@aws-cdk/aws-s3-deployment": "^1.103.0",
    "@aws-cdk/core": "^1.103.0",
    "@aws-cdk/custom-resources": "^1.103.0",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.16",
    "standard-version": "^9.3.0"
  }
}
